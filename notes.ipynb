   "source": [
    "import numpy as np\n",
    "from math import factorial\n",
    "import sympy\n",
    "x=sympy.Symbol('x')\n",
    "Phi=np.sum([(factorial(n))\n",
    "            *(np.power(x,(2*n))) for n in range(3)])\n",
    "sympy.Derivative(Phi,x, evaluate=True)"
   ]
